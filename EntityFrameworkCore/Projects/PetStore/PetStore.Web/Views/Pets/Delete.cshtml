@model PetDetailsServiceModel

<h1>Are you sure you want to delete the following pet</h1>

<div>
	<p>@Model.Category</p>
	<p>@Model.Breed</p>
	<p>@Model.DateOfBirth</p>
	<p>@Model.Gender</p>
	<p>@Model.Description</p>
	<p>@Model.Price</p>
</div>

<div>
	<a href="/Pets/ConfirmDelete/@Model.Id" class="btn btn-danger">Yes</a> &nbsp; <a href="/Pets/All" class="btn btn-info">No</a>
</div>